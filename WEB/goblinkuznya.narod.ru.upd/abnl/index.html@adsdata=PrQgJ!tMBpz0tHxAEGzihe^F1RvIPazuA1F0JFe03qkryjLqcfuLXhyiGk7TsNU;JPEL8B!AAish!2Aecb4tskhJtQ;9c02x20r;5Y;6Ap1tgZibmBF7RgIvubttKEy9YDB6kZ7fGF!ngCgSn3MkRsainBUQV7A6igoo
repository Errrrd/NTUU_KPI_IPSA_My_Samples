function resizeDiv(){
	var WX,WY,BX,BY;
	var o=document.getElementById("dVa4ZHMt4"),t,d;
	if (!o) return;
	if (!(t=document.getElementById("wrapperXa4ZHMt4"))) WX=0;
	else WX=t.value;
	if (!(t=document.getElementById("wrapperYa4ZHMt4"))) WY=0;
	else WY=t.value;
	if (!(t=document.getElementById("bannerXa4ZHMt4"))) BX=0;
	else BX=t.value;
	if (!(t=document.getElementById("bannerYa4ZHMt4"))) BY=0;
	else BY=t.value;
	d=document.getElementById("mainadsdva4ZHMt4");
	if (d) {
		if (BX<0) d.style.width="100%";
		else if (BX>0) d.style.width=BX+"px";
		if (BY<0) d.style.height="100%";
		else if (BY>0) d.style.height=BY+"px";
	}
	BX=parseInt(BX)+parseInt(WX);
	BY=parseInt(BY)+parseInt(WY);
	if (BX<0) o.style.width="100%";
	else if (BX>0) o.style.width=BX+"px";
	if (BY<0) o.style.height="100%";
	else if (BY>0) o.style.height=BY+"px";
	o.style.display='';
}
