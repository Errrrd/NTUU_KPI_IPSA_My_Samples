function resizeDiv(){
	var WX,WY,BX,BY;
	var o=document.getElementById("dVazaL7uf"),t,d;
	if (!o) return;
	if (!(t=document.getElementById("wrapperXazaL7uf"))) WX=0;
	else WX=t.value;
	if (!(t=document.getElementById("wrapperYazaL7uf"))) WY=0;
	else WY=t.value;
	if (!(t=document.getElementById("bannerXazaL7uf"))) BX=0;
	else BX=t.value;
	if (!(t=document.getElementById("bannerYazaL7uf"))) BY=0;
	else BY=t.value;
	d=document.getElementById("mainadsdvazaL7uf");
	if (d) {
		if (BX<0) d.style.width="100%";
		else if (BX>0) d.style.width=BX+"px";
		if (BY<0) d.style.height="100%";
		else if (BY>0) d.style.height=BY+"px";
	}
	BX=parseInt(BX)+parseInt(WX);
	BY=parseInt(BY)+parseInt(WY);
	if (BX<0) o.style.width="100%";
	else if (BX>0) o.style.width=BX+"px";
	if (BY<0) o.style.height="100%";
	else if (BY>0) o.style.height=BY+"px";
	o.style.display='';
}
